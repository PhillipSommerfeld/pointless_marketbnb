<h1>All Items For Sale</h1>

<%@offers.each do |offer| %>
<div class="card-trip">
  <img src="<%=offer.image_url%>" />
  <div class="card-trip-infos">
    <div>
      <h2><%=offer.item_name%></h2>
      <p><%=offer.description%></p>
    </div>
    <h2 class="card-trip-pricing"><%=offer.price%></h2>
  </div>
</div>
<%end%>
